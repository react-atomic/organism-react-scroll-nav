{
    "env": {
        "build": {
            "presets": [
              "env",
              "react",
              "stage-0"
            ],
            "plugins": [
              "add-module-exports",
              "transform-object-assign",
              "transform-react-constant-elements"
            ]
        },
        "es": {
            "presets": [
              ["env", {"modules": false}],
              "react",
              "stage-0"
            ],
            "plugins": [
              "transform-object-assign",
              "transform-react-pure-class-to-function",
              "transform-react-constant-elements"
            ]
        }
    }
}
